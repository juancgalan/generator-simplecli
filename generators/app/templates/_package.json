{
    "name": "<%= appname %>",
    "version": "0.1.0",
    "description": "",
    "main": "src/main/app.js",
    "scripts": {
        "run": "npm src/main/app.js",
        "test": "jest --watchAll",
        "prepublishOnly": "nsp check",
        "pretest": "eslint .",
        "precommit": "lint-staged"
    },
    "keywords": ["cli"],
    "author": "<%= authorname %>",
    "devDependencies": {
        "babel-preset-env": "^1.7.0",
        "jest": "^23.2.0",
        "husky": "^0.14.3",
        "prettier": "^1.13.6",
        "eslint-plugin-prettier": "^2.6.1",
        "eslint-config-prettier": "^2.9.0",
        "lint-staged": "^7.2.0",
        "nsp": "^3.2.1",
        "eslint": "^5.0.0"
    },
    "dependencies": {
        "chalk": "^2.4.1",
        "inquirer": "^6.0.0"
    },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "<%= repository %>",
  "license": "<%= license %>"
}